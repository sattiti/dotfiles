# vim: ft=sh fenc=utf-8

# cmd {{{
# ls
alias ls="ls -G"
alias la="ls -GlaFht"
# }}}

# path to {{{
# path to dev directory.
alias dev="cd $HOME/Documents/dev"

# path to desktop.
alias dt="cd $HOME/Desktop"
# }}}

# Application shortcut {{{
alias sa="open /Applications/Safari.app"
alias ff="open /Applications/Firefox.app"
alias gc="open /Applications/Google\ Chrome.app"
alias music="open /Applications/Music.app"
# }}}