[core]
  autocrlf = false
  editor = vim
  filemode = true
  ignorecase = false
  longpaths = true
  pager = less -r
  quotepath = false

[color]
  ui = true

[alias]
  aa = add .
  br = branch -l -v
  ch = checkout
  co = commit -m
  fe = fetch -v --all -p
  fl = diff --name-only
  get = pull -v --progress --no-rebase
  plog = log --oneline --stat-width=110 --decorate=full --date=default --pretty=tformat:\"%C(white magenta bold)%s%C(reset)%x20%C(yellow red bold reverse)%d%C(reset)%n%C(yellow)%H%C(reset)%x20%C(cyan)%ad%C(reset)%x20%C(green italic)%an%x20<%aE>%C(reset)%n%n\" 
  st = status -v
  up = push -v --progress -u --prune

