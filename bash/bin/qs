set -eu

# func
usage(){
  local cmdname=$(basename $0)
  cat <<- EOL
Usage:
$cmdname QUERY_STRING
EOL
  unset cmdname
  exit 1
}

err(){
  echo "No such directory."
  exit 1
}

# exit if args is not 1.
if [ $# -ne 1 ]; then
  usage
fi

querystring=$1

echo $querystring | tr "?" "\n" | tr "&" "\n" | column -t -s=